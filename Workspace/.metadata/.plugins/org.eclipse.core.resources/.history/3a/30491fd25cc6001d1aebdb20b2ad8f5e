package com.skillstorm.general2;

public class Knight extends Player{
	
	public Knight() {}
	
	public Knight(String name, int age, String type, String power,
			double level, int hp, String weapon) {
		super(name, age, type, power, level, hp, weapon);
	}
	
	public void telikinisis() {
		System.out.println(getName() + " used her " + getPower());
	}
	
	public void fly() {
		
	}

}
