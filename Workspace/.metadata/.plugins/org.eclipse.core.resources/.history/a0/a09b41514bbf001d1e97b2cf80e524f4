package com.skillstorm.general;

public class MyOwnNotes {
	
	public static void main(String[] args) {
		
		int i = 1;
		while(i <= 5) {
			System.out.println("Hello " + i);
			i++;
		}

	}
	public static void usingArray() {
	
	int[] nums = {3, 6, 8 };

	int count = 0;
	while (count < nums.length) {
	System.out.println(nums[count]);
	count++;
	}
	}

}
