package com.skillstorm.general;

public class MyOwnNotes {
	
	public static void main(String[] args) {
		
		int i = 1;
		while(i <= 5) {
			System.out.println("Hello " + i);
			i++;
			
			System.out.print(usingArray());
		}

	}
	public static void usingArray() {
		
		int[] myNumbers = new int[3];
		myNumbers[0] = 3;
		myNumbers[1] = 4;
		myNumbers[2] = 5;
		myNumbers[3] = 6;
		myNumbers[4] = 7;
		
		System.out.println(myNumbers[4]);
		
	}
	


}
