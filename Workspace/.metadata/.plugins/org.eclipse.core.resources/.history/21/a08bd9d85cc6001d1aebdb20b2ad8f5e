package com.skillstorm.general2;

public class Padawan extends Player{
	
	public Padawan() {}

	public Padawan(String name, int age, String type, String power,
			double level, int hp, String weapon) {
		super(name, age, type, power, level, hp, weapon);
	}
	
	public void fly() {
		
	}
	
	@Override
	public void force() {
		System.out.println(getName() + " casted a spell!");
	}
	
	public void mindRead() {
		System.out.println(getName() + " can read minds!");
	}
	
}
